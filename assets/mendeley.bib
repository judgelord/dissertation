@article{Howell2002b,
abstract = {Scholars have largely ignored one of the most important ways in which presidents influence the administrative state in the modern era, that is, by creating administrative agencies through executive action. Because they can act unilaterally, presidents alter the kinds of administrative agencies that are created and the control they wield over the federal bureaucracy. We analyze the 425 agencies established between 1946 and 1995 and find that agencies created by administrative action are significantly less insulated from presidential control than are agencies created through legislation. We also find that the ease of congressional legislative action is a significant predictor of the number of agencies created by executive action. We conclude that the very institutional factors that make it harder for Congress to legislate provide presidents new opportunities to create administrative agencies on their own, and to design them in ways that maximize executive control.

Lewis and Howell examine 425 new agencies created between 1946 and 1995 (1096) by the exercise of unilateral executive action and de monstrate that bureaucracies created under the guise of executive action are much more susceptible to p residential control. They also find an inverse relationship between congressional strength and unilateral activity, meaning that when Congress is stronge st, the executive is much less likely to act unilaterally. As they state, “these findings suggest that the very institutional factors that undermine Congress augment presidential influence and control (1096).” This work marks an interesting change in direc tion for the presidential literature, which prior to this article has focused primarily on presidential control of agencies already in existence. },
author = {Howell, William G. and Lewis, David E.},
doi = {10.1111/1468-2508.00164},
file = {::},
journal = {The Journal of Politics},
month = {nov},
number = {4},
pages = {1095--1114},
publisher = {Blackwell Publishing, Inc.},
title = {{Agencies by Presidential Design}},
url = {http://www.journals.uchicago.edu/doi/10.1111/1468-2508.00164},
volume = {64},
year = {2002}
