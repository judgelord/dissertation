volume = {40},
year = {2012}
}
@book{Lewis2008,
abstract = {Lewis positions his work in the aftermath of Hurricane Katrina, and examines the way in which presidents use and appoint political bureaucratic appointees, and how these political appointees affect bureaucratic efficacy. This work is a follow up to the earlier work of Moe and Howell (1999), among others. One of the central conclusions is that agencies with greater numbers of political appointees have consistently lower Program Assessment Rating Tool grades, which speaks to the impact of politicization on bureaucratic efficacy and competence. Lewis' work is also noteworthy because of the data sets he employed in his analysis; he leverages the Office of Personnel Management's personnel data files to mak e the distinction between career civil servants and political appointees. Reviews of the work have noted that this constitutes the most comprehensive inventory of political appointees at the federal level (William Howell, Political Science Quarterly, 182).

