@unpublished{Brockway2018,
abstract = {Distrust in intellectuals has pervaded American culture for centuries. This distrust, however, has
not led to a similar distrust in the scientific community. Aggregate trust and belief in science has
remained relatively stable over time. Recently, however, a partisan divide has formed in
judgements about science, the scientific community, and science issues such as policies
concerning climate change. Research on the relationship between science and politics has
focused on how partisan affiliations shape individual judgements about these scientific issues
and finds that partisan affiliations strongly influence an individual's judgments about science
issues and science-related policies. These efforts assume that partisan affiliations shape scientific
judgements. But attitudes about science may be more embedded than previously thought. I argue
that an individual's belief about the merits of science, called science efficacy, is a deep-seated,
central worldview. Instead of being a result of partisan affiliations, I find that science efficacy
consistently shapes partisan affiliations. Using longitudinal cross-sectional and panel survey
data, I demonstrate that science efficacy has a significant unidirectional impact on individual
partisanship and that this relationship is robust to accounting for science-based policy positions.
Taken together these results suggest science efficacy is central to partisan identity and is
increasingly important in the American political landscape.},
author = {Brockway, Mark},
institution = {APSA},
title = {{Science Efficacy in American Politics}},
url = {https://convention2.allacademic.com/one/apsa/apsa18/index.php?cmd=Online+Program+Download+Document{\&}document{\_}type=document{\&}key=online{\_}program{\_}view{\_}paper{\_}downloads{\&}document{\_}key=c7a1cebd18ea38152468d2e68d6fa6ab{\&}filename=apsa18{\_}proceeding{\_}1386160.pdf{\&}PHPSESSI},
year = {2018}
