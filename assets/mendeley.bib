@unpublished{Furnas2019,
abstract = {This article considers the implications of lobbying firms operating as strategic actors in the party system in the United States. It states a partisan theory of legislative subsidy in which members of Congress are inclined to meet with lobbyists when they share a partisan affiliation. It formulates hypotheses that lobbying firms receive greater revenues by being tied to the majority party in Congress and that they are likely to receive greater revenues from being aligned with the House of Representatives than the Senate. These hypotheses are tested using data available under the Lobbying Disclosure Act from 2008 to 2016. Regression analysis reveals that lobbying firms receive financial benefits when they have partisan ties with the majority party in the House but not with the majority party in the Senate. The results indicate that there are greater financial returns to lobbying the House than the Senate. A difference-indifferences analysis establishes that Republican-aligned lobbying firms received a financial windfall when the Republican Party reclaimed the House in 2011 but not when the Republicans reclaimed the Senate in 2015. The outcome of the 2018 midterm congressional election presents another opportunity to test these hypotheses once the 2019 lobbying data become available.},
author = {Furnas, Alexander C and Heaney, Michael T and Lapira, Timothy M},
file = {::},
title = {{The Partisan Ties of Lobbying Firms}},
