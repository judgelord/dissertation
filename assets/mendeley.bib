pages = {1789--1865},
title = {{Yale Law School Yale Law School Legal Scholarship Repository Faculty Scholarship Series Yale Law School Faculty Scholarship}},
url = {http://digitalcommons.law.yale.edu/fss{\_}papers/5179},
volume = {115},
year = {2015}
}
