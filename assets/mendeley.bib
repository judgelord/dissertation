doi = {10.1177/1086026619858857},
issn = {1086-0266},
journal = {Organization {\&} Environment},
keywords = {Sustainable Development Goals (SDGs),corporate political activity,environmental attitudes and behavior,environmental governance and regulation,institutional theory,organizational behavior and the environment,paradox theory,sustainable development,theoretical approach},
