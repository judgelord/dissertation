@article{Cook2016,
abstract = {The worries American citizens express about the distorting influence of minority factions, or special interests, on public governance are as old as the republic. I briefly recount the constitutional design flaws and developmental changes that have combined to make these worries more valid than ever in an age of administratively centered governance. I then review the evidence on the nature and extent of special interest influence on the administrative process from a broad swath of scholarship. After noting the limited efficacy of attempts to limit special interest influence via minor tinkering with the separation of powers, I argue for more fundamental structural change in the form of administration as a separate, constitutionally recognized element of the separation of powers. I outline basic structural features and offer hypothetical yet plausible consequences of such a design change. The latter, I contend, are amenable to confirmation or refutation through systematic analysis of evidence that is already available from past and current governing experience. Keywords special interests, administrative power, constitutional redesign During the 1912 campaign for the U.S. presidency, Theodore Roosevelt and Woodrow Wilson engaged in a spirited debate about how the nation should cope with the trusts, those massive corporate combinations that had emerged in the previous two decades and gained control of markets in a host of vital products and services. Roosevelt called for regulating the trusts through a national commission placed in the executive branch because the effort to break up the trusts through the courts had failed. He insisted that regulating rather than dismantling the trusts would ensure that the competitive advantages of their organizational efficiencies would be preserved. Regulating competition rather than regulating the trusts, Wilson countered, was the better strategy. He contended that regulating the trusts by a commission of experts would be tantamount to allowing corporate monopolies to control the government, because most of the expertise for the regulatory commission would come from the regulated entities. Wilson warned against placing unguided discretion in the hands of such experts. Instead, he called for designing the law regulating competition with sufficient specificity and clarity as to limit administrative discretion, which would ensure that law and not the power of monopoly expertise would prevail.},
author = {Cook, Brian J},
doi = {10.1177/0275074015612160},
file = {::},
journal = {American Review of Public Administration},
number = {1},
pages = {3--27},
title = {{Curing the Mischiefs of Faction in the American Administrative State}},
url = {https://journals.sagepub.com/doi/pdf/10.1177/0275074015612160},
volume = {46},
year = {2016}
}
@article{Rosenbloom2000a,
abstract = {One of the twentieth century's "big questions" for United States government has been how best to
 retrofit, or integrate, the full-fledged federal administrative state into the constitutional scheme. The
 public administration orthodoxy initially advocated placing the executive branch almost entirely
 under presidential control; Congress and the federal judiciary responded otherwise. Congress
 decided to treat the agencies as its extensions for legislative functions and to supervise them more
 closely. The courts developed an elaborate framework for imposing constitutional rights, values,
 and reasoning on public administration practice. As the challenge of retrofitting continues into the
 twenty-first century, public administrators might profitably play a larger role in the constitutional
 discourse regarding the administrative state's place in constitutional government.},
