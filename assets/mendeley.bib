year = {2017}
}
@article{Schaltegger2017,
abstract = {The “business case for sustainability” is a notion often referenced in the corporate sustainability and corporate social responsibility literature. Whereas some see sustainability and the business case as contradictions and thus emphasize the existence of trade-offs, others highlight how (potential) business cases can be created by managing ecological, social, and economic aspects. Both views have in common that the “business case” is implicitly or explicitly seen as creating financial performance, often for one group of stakeholders, only. The fact that a business case is not a given phenomenon but has to be co-created in the exchange between and with contributions from various stakeholders has so far not been analysed in depth. By taking a stakeholder theory perspective, this article extends the existing research on what business and a business case are about and analyses the understanding of business cases for sustainability and how they can be created with and by stakeholders.},
author = {Schaltegger, Stefan and H{\"{o}}risch, Jacob and Freeman, R. Edward},
doi = {10.1177/1086026617722882},
issn = {1086-0266},
journal = {Organization {\&} Environment},
keywords = {business cases,business cases for sustainability,business performance,corporate sustainability,management of stakeholder relationships,stakeholder business case for sustainability,stakeholder theory,sustainability management,sustainability performance,triple win},
month = {aug},
pages = {108602661772288},
publisher = {SAGE PublicationsSage CA: Los Angeles, CA},
title = {{Business Cases for Sustainability: A Stakeholder Theory Perspective}},
url = {http://journals.sagepub.com/doi/10.1177/1086026617722882},
year = {2017}
}
@article{Morales-Raya2018,
abstract = {Although the literature has shown the benefits of corporate environmental reputation, little is known about how this reputation develops. Based on impression management theory, we examine how a firm's environmental practices create its environmental reputation. We undertake a fine-grained analysis by distinguishing between high and low environmental external visibility practices and examining the effect of decoupling environmental practices (i.e., using only practices that are highly visible) on environmental reputation. We also propose that corporate environmental disclosure moderates these relationships. The hypotheses are tested on a sample of 241 U.S. firms included in Newsweek's Green Rankings. Our results suggest that managers should avoid decoupling because such practices heavily jeopardize environmental reputation. In contrast, developing a balanced set of both high and low environmental external visibility practices, enhanced through environmental disclosure, pays for environmental reputation.},
author = {Morales-Raya, Matilde and Mart{\'{i}}n-Tapia, Inmaculada and Ortiz-de-Mandojana, Natalia},
doi = {10.1177/1086026617753154},
issn = {1086-0266},
journal = {Organization {\&} Environment},
keywords = {corporate environmental reputation,decoupling practices,environmental disclosure,environmental practices,impression management,visibility},
month = {feb},
