@article{Horisch2014,
abstract = {This essay examines links, similarities, and dissimilarities between stakeholder theory and sustainability management. Based on the analysis a conceptual framework is developed to increase the applicability and the application of stakeholder theory in sustainability management. Concluding from the analysis, we identify three challenges of managing stakeholder relationships for sustainability: strengthening the particular sustainability interests of stakeholders, creating mutual sustainability interests based on these particular interest, and empowering stakeholders to act as intermediaries for nature and sustainable development. To address these challenges three interrelated mechanisms are suggested: education, regulation, and sustainability-based value creation for stakeholders.},
